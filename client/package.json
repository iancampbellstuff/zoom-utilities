{
  "name": "@zoom-utilities/client",
  "description": "A Quasar app for simplifying Zoom tasks.",
  "productName": "Zoom Utilities Client",
  "author": "iancampbellstuff",
  "version": "1.0.0",
  "license": "ISC",
  "private": true,
  "scripts": {
    "build-electron": "npx --no-install quasar build -m electron -T all -t mat",
    "build-pwa": "npx --no-install quasar build -m pwa -T all -t mat",
    "build": "npm run build-pwa",
    "clean": "npx --no-install quasar clean && npx --no-install rimraf coverage dist src-electron src-pwa",
    "format": "npx --no-install prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "lint": "npx --no-install eslint --ext .js,.ts,.vue ./",
    "prebuild": "npm run lint && npm run clean",
    "prestart": "npx --no-install rimraf src-electron src-pwa",
    "pretest": "npm run lint && npx --no-install rimraf coverage",
    "start-electron": "NODE_OPTIONS='--abort-on-uncaught-exception --trace-deprecation --trace-warnings' npx --no-install quasar dev -m electron -t mat",
    "start-pwa": "NODE_OPTIONS='--abort-on-uncaught-exception --openssl-legacy-provider --trace-deprecation --trace-warnings' npx --no-install quasar dev -m pwa -t mat",
    "start": "npm run start-pwa",
    "serve": "npx --no-install quasar serve dist/ --port 8080",
    "test-only": "npx --no-install jest --colors --coverage",
    "test-open": "npm test && open coverage/lcov-report/index.html",
    "test-serve": "npx --no-install quasar serve coverage/lcov-report/ --port 8788",
    "test-watch": "npm test -- --watchAll",
    "test": "npm run test-only",
    "update-snapshots": "npx --no-install jest --updateSnapshot"
  },
  "dependencies": {
    "@quasar/extras": "^1.16.12",
    "axios": "^0.28.1",
    "pinia": "^2.2.2",
    "quasar": "^2.16.9",
    "vue": "^3.4.38",
    "vue-router": "^4.4.3"
  },
  "devDependencies": {
    "@pinia/testing": "^0.1.5",
    "@quasar/app-webpack": "^3.13.4",
    "@quasar/quasar-app-extension-testing-unit-jest": "^3.0.2",
    "@types/axios": "^0.14.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.106",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vue/test-utils": "^2.4.6",
    "electron": "^32.0.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-jest": "^27.9.0",
    "eslint-plugin-vue": "^9.27.0",
    "jest": "^29.7.0",
    "nock": "^13.5.5",
    "prettier": "^2.8.8",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.2.5",
    "workbox-webpack-plugin": "^7.1.0"
  },
  "browserslist": [
    "last 10 Chrome versions",
    "last 10 Firefox versions",
    "last 4 Edge versions",
    "last 7 Safari versions",
    "last 8 Android versions",
    "last 8 ChromeAndroid versions",
    "last 8 FirefoxAndroid versions",
    "last 10 iOS versions",
    "last 5 Opera versions"
  ],
  "engines": {
    "node": "20.x",
    "npm": ">=10.x"
  }
}
