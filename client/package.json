{
  "name": "@zoom-utilities/client",
  "description": "A Quasar app for simplifying Zoom tasks",
  "productName": "Zoom Utilities Client",
  "author": "iancampbellstuff",
  "version": "1.0.0",
  "license": "ISC",
  "private": true,
  "scripts": {
    "build-electron": "npx --no-install quasar build -m electron -T all -t mat",
    "build-pwa": "npx --no-install quasar build -m pwa -T all -t mat",
    "build": "npm run build-pwa",
    "clean": "npx --no-install quasar clean && npx --no-install rimraf dist src-electron src-pwa",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "lint": "eslint --ext .js,.ts,.vue ./",
    "prebuild": "npm run lint && npm run clean",
    "prestart": "npx --no-install rimraf src-electron src-pwa",
    "pretest": "npm run lint && npx --no-install rimraf coverage",
    "start-electron": "npx --no-install quasar dev -m electron -t mat",
    "start-pwa": "npx --no-install quasar dev -m pwa -t mat",
    "start": "NODE_OPTIONS=--openssl-legacy-provider npm run start-pwa",
    "serve": "npx --no-install quasar serve dist/ --port 8080",
    "test-only": "jest --coverage",
    "test-serve": "npx --no-install quasar serve coverage/lcov-report/ --port 8788",
    "test-watch": "npm test -- --watchAll",
    "test": "npm run test-only",
    "update-snapshots": "jest --updateSnapshot"
  },
  "dependencies": {
    "@quasar/extras": "^1.16.9",
    "axios": "^0.27.2",
    "core-js": "^3.34.0",
    "pinia": "^2.1.7",
    "quasar": "^2.14.2",
    "vue": "^3.3.13",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@pinia/testing": "^0.1.3",
    "@quasar/app": "^3.3.3",
    "@quasar/app-vite": "^1.7.1",
    "@quasar/babel-preset-app": "^2.0.2",
    "@quasar/quasar-app-extension-testing-unit-jest": "^3.0.0-alpha.10",
    "@types/jest": "^29.5.11",
    "@types/node": "^12.20.55",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vue/test-utils": "^2.4.3",
    "autoprefixer": "^10.4.16",
    "electron": "^23.3.13",
    "electron-packager": "^15.5.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-vue": "^9.19.2",
    "jest": "^29.7.0",
    "nock": "^13.4.0",
    "prettier": "^2.8.8",
    "sass": "^1.33.0",
    "workbox-build": "^6.6.0",
    "workbox-cacheable-response": "^6.6.0",
    "workbox-core": "^6.6.0",
    "workbox-expiration": "^6.6.0",
    "workbox-precaching": "^6.6.0",
    "workbox-routing": "^6.6.0",
    "workbox-strategies": "^6.6.0",
    "workbox-webpack-plugin": "^6.6.0"
  },
  "browserslist": [
    "last 10 Chrome versions",
    "last 10 Firefox versions",
    "last 4 Edge versions",
    "last 7 Safari versions",
    "last 8 Android versions",
    "last 8 ChromeAndroid versions",
    "last 8 FirefoxAndroid versions",
    "last 10 iOS versions",
    "last 5 Opera versions"
  ],
  "engines": {
    "node": "^24 || ^22 || ^20 || ^18 || ^16 || ^14.19",
    "npm": ">= 6.14.12",
    "yarn": ">= 1.17.3"
  }
}
