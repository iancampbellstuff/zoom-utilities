{
  "name": "zoom-utilities",
  "version": "0.0.1",
  "description": "A Quasar app for simplifying Zoom tasks",
  "productName": "Zoom Utilities",
  "author": "icampbell2",
  "private": true,
  "scripts": {
    "build": "npx --no-install quasar build -m electron -T all -t mat",
    "clean": "npx --no-install quasar clean && npx --no-install rimraf .quasar dist src-electron",
    "lint": "eslint --fix --ext .js,.ts,.vue ./",
    "prebuild": "npm run lint && npm run clean",
    "pretest": "npm run lint && npx --no-install rimraf coverage",
    "start": "npx --no-install quasar dev -m electron -t mat",
    "test-only": "jest --updateSnapshot --colors",
    "test-watch": "npm test -- --watchAll",
    "test": "npm run test-only"
  },
  "dependencies": {
    "@quasar/extras": "^1.0.0",
    "axios": "^0.21.1",
    "core-js": "^3.6.5",
    "quasar": "^1.0.0",
    "vue-class-component": "^7.2.2",
    "vue-property-decorator": "^8.3.0"
  },
  "devDependencies": {
    "@quasar/app": "^2.0.0",
    "@quasar/quasar-app-extension-testing-unit-jest": "2.2.2",
    "@types/jest": "26.0.23",
    "@types/node": "^10.17.15",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "babel-eslint": "^10.0.1",
    "devtron": "1.4.0",
    "electron": "9.4.4",
    "electron-debug": "3.2.0",
    "electron-devtools-installer": "3.2.0",
    "electron-packager": "14.2.1",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-vue": "^7.7.0",
    "husky": "6.0.0",
    "jest-serializer-vue": "2.0.2",
    "typescript": "4.3.4"
  },
  "browserslist": [
    "last 10 Chrome versions",
    "last 10 Firefox versions",
    "last 4 Edge versions",
    "last 7 Safari versions",
    "last 8 Android versions",
    "last 8 ChromeAndroid versions",
    "last 8 FirefoxAndroid versions",
    "last 10 iOS versions",
    "last 5 Opera versions"
  ],
  "engines": {
    "node": ">= 10.18.1",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  },
  "jest": {
    "snapshotSerializers": [
      "jest-serializer-vue"
    ]
  },
  "husky": {
    "hooks": {
      "pre-push": "npm test"
    }
  }
}
