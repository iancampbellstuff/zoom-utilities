{
  "name": "@zoom-utilities/server",
  "description": "An Express app for simplifying Zoom tasks.",
  "productName": "Zoom Utilities Server",
  "author": "icampbell2",
  "license": "ISC",
  "private": true,
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "clean": "npx --no-install rimraf dist",
    "lint-fix": "npm run lint -- --fix",
    "lint": "eslint --ext .js,.ts ./",
    "prebuild": "npm run lint && npm run clean",
    "pretest": "npm run lint && npx --no-install rimraf coverage",
    "start": "ts-node-dev --respawn --transpile-only src/index.ts",
    "test-only": "jest --colors",
    "test-watch": "npm test -- --watchAll",
    "test": "npm run test-only"
  },
  "devDependencies": {
    "@types/axios": "0.14.0",
    "@types/compression": "1.7.2",
    "@types/cors": "2.8.13",
    "@types/express": "4.17.17",
    "@types/express-validator": "3.0.0",
    "@types/jest": "27.5.2",
    "@types/jsonwebtoken": "8.5.9",
    "@types/node": "16.18.12",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.33.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-jest": "24.7.0",
    "jest": "27.5.1",
    "ts-jest": "27.1.5",
    "ts-node-dev": "1.1.8",
    "typescript": "4.9.5"
  },
  "dependencies": {
    "axios": "0.27.2",
    "compression": "1.7.4",
    "cors": "2.8.5",
    "express": "4.18.2",
    "express-validator": "6.15.0",
    "jsonwebtoken": "8.5.1"
  }
}
