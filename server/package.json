{
  "name": "@zoom-utilities/server",
  "description": "An Express app for simplifying Zoom tasks.",
  "productName": "Zoom Utilities Server",
  "author": "icampbell2",
  "license": "ISC",
  "private": true,
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "clean": "npx --no-install rimraf dist",
    "lint-fix": "npm run lint -- --fix",
    "lint": "eslint --ext .js,.ts ./",
    "prebuild": "npm run lint && npm run clean",
    "pretest": "npm run lint && npx --no-install rimraf coverage",
    "start": "ts-node-dev --respawn --transpile-only src/index.ts",
    "test-only": "jest --colors",
    "test-watch": "npm test -- --watchAll",
    "test": "npm run test-only"
  },
  "devDependencies": {
    "@types/axios": "0.14.0",
    "@types/compression": "1.7.0",
    "@types/express": "4.17.12",
    "@types/express-validator": "3.0.0",
    "@types/jest": "26.0.23",
    "@types/node": "16.0.0",
    "@typescript-eslint/eslint-plugin": "4.28.1",
    "@typescript-eslint/parser": "4.28.1",
    "eslint": "7.30.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-jest": "24.3.6",
    "jest": "27.0.6",
    "ts-jest": "27.0.3",
    "ts-node-dev": "1.1.8",
    "typescript": "4.3.5"
  },
  "dependencies": {
    "axios": "0.21.1",
    "compression": "1.7.4",
    "express": "4.17.1",
    "express-validator": "6.12.0"
  }
}
