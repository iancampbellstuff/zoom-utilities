{
  "name": "@zoom-utilities/server",
  "description": "An Express app for simplifying Zoom tasks.",
  "productName": "Zoom Utilities Server",
  "author": "iancampbellstuff",
  "version": "1.0.0",
  "license": "ISC",
  "private": true,
  "scripts": {
    "build": "npx --no-install tsc --project tsconfig.build.json",
    "clean": "npx --no-install rimraf dist",
    "lint-fix": "npm run lint -- --fix",
    "lint": "npx --no-install eslint --ext .js,.ts ./",
    "prebuild": "npm run lint && npm run clean",
    "pretest": "npm run lint && npx --no-install rimraf coverage",
    "start": "npx --no-install cross-env NODE_OPTIONS='--abort-on-uncaught-exception --trace-deprecation --trace-warnings' npx --no-install ts-node-dev --respawn --transpile-only src/index.ts",
    "test-only": "npx --no-install jest --colors --coverage",
    "test-open": "npm test && open coverage/index.html",
    "test-watch": "npm test -- --watchAll",
    "test": "npm run test-only"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.106",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-jest": "^27.9.0",
    "jest": "^27.5.1",
    "nock": "^13.5.5",
    "rimraf": "^6.0.1",
    "ts-jest": "^27.1.5",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "axios": "^0.28.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "express-validator": "^6.15.0"
  },
  "engines": {
    "node": "20.x",
    "npm": ">=10.x"
  }
}
